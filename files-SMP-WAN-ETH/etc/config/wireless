config wifi-device 'wl000'
	option type 'ralink'
	option channel '36'
	option legacy_rates '0'
	option htmode 'VHT40'
	option disabled '0'

config wifi-iface 'default_wl000'
	option device 'wl000'
	option mode 'ap'
	option ssid 'EasyBOX-5GHz'
	option encryption 'psk2+aes'
	option key 'WiFipassword'
	option network 'lan'
	option disabled '0'

config wifi-device 'wl010'
	option type 'ralink'
	option channel '11'
	option legacy_rates '0'
	option htmode 'VHT40'
	option disabled '0'

config wifi-iface 'default_wl010'
	option device 'wl010'
	option mode 'ap'
	option ssid 'EasyBOX-2GHz'
	option encryption 'psk2+aes'
	option key 'WiFipassword'
	option network 'lan'
	option disabled '0'

